    <!-- ############## -->
    <!-- TOUCH SETTINGS -->
    <!-- ############## -->
    <!-- 
	     QueueInterval: (long)
	     Interval in milliseconds to check for email queue for new mails to send.
	     Default: 10000
		-->
    <add key="Touch.QueueInterval" value="10000"/>
    <!-- 
	     ThrottleObject: (string)
	     Optional throttle to apply to Touch requests (id).
	     Specify object="RuleID" factor="AmountToThrottle". 
	     The amount to throttle must be a positive integer.
	     The actual throttling amount depends on the Touch.QueueInterval setting.
	     Default: none
	     
	     Example: object="1" factor="10" slows down RuleId 1 by 10x.
		-->
    <add key="Touch.ThrottleObject" object="" factor=""/>
    <!-- Specify which mail service to use [MassMailer|Komunikator] -->
