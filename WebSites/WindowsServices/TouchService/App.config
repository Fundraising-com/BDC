    <!-- ############### -->
    <!-- COMMON SETTINGS -->
    <!-- ############### -->
    <!-- 
			 ERROR LOGGING. 
			 Specify where to log errors. You may specify more than one location.
			 Valid logType is "email", "eventlog" or "file".
			 Valid logSource is either the email address for "email" type, source name
			 for "eventlog" type or file name for "file" type.
			 For "eventlog" type, you may need to create a new Key with the source name 
			 in the registry under HKLM\SYSTEM\CurrentControlSet\Services\Eventlog\Application
		-->
    <add key="Common.Log.Settings.Bulk" maxLog="10" interval="600000"/>
    <add key="Common.Log.Error" logType="email" logSource="bdcdevelopers@fundraising.com" onBulk="discard"/>
    <add key="Common.Log.Error" logType="file" logSource="C:\Log\Touch_%yyyy%%MM%%dd%_Log.xml"/>
    <add key="Common.Log.Warn" logType="file" logSource="C:\Log\Touch_%yyyy%%MM%%dd%_Log.xml"/>
    <add key="Common.Log.Info" logType="file" logSource="C:\Log\Touch_%yyyy%%MM%%dd%_Log.xml"/>
    <add key="Common.Log.Debug" logType="file" logSource="C:\Log\Touch_%yyyy%%MM%%dd%_Log.xml"/>
