@using System.Configuration

<!DOCTYPE html>
<html lang="en" ng-app="ezfund">
<head>
   <meta name="viewport" content="width=device-width, initial-scale=1">
   @if (ViewBag.UseAngularJSMetaData != null && bool.Parse(ViewBag.UseAngularJSMetaData.ToString()))
   {
      <title ng-bind="MetaService.metaTitle()"></title>
      <meta name="description" content="{{ MetaService.metaDescription() }}" />
      <meta name="keywords" content="{{ MetaService.metaKeywords() }}" />
      <meta name="canonicalUrl" content="{{ MetaService.metaCanonicalUrl() }}" />
   }
   else
   {
      <title>@ViewBag.Title</title>
      <meta name="description" content="@ViewBag.Description" />
      <meta name="keywords" content="@ViewBag.Keywords" />
   }


