                        <span style="color: #087A92; right:17px; bottom:15px; position:relative;" class="circled-text" ng-show="shoppingCartCtrl.scope.$storage.ShoppingCart.Items.length > 0">{{shoppingCartCtrl.scope.$storage.ShoppingCart.Items.length}}</span>
