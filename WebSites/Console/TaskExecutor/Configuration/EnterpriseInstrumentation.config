      <eventSource name="SWCorporate.SystemEx.Messaging.MessageQueueConfiguredEnumerator" type="softwareElement" internalExceptionHandler="ignore" description="SW Corporate Messaging Extensions Message Queue Enumerator Factory" />
      <eventSource name="SWCorporate.SystemEx.Messaging.FileSystemWatcherAgent" type="softwareElement" internalExceptionHandler="ignore" description="SW Corporate Messaging Extensions File-System Watcher Agent Event Source" />
