<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns="http://www.microsoft.com/schema/EnterpriseInstrumentation/v1/EnterpriseInstrumentation.xsd">
   <instrumentedApp name="GA.BDC.Console.TaskExecutor" mode="released" />
   <eventSources>
      <eventSource name="GA.BDC.Console.TaskExecutor.BusinessLogic.FRIDocPublisher" type="softwareElement" internalExceptionHandler="ignore" description="GA BDC Order Payment Publisher" />
     <eventSource name="GA.BDC.Console.TaskExecutor.BusinessLogic.EZIDocPublisher" type="softwareElement" internalExceptionHandler="ignore" description="GA EZFUND Order Payment Publisher" />
    <eventSource name="GA.BDC.Console.TaskExecutor.Tasks.ReceiveOrdersTask" type="softwareElement" internalExceptionHandler="ignore" description="GA BDC Order Confirmation Receiver" />
     <eventSource name="GA.BDC.Console.TaskExecutor.Tasks.ReceiveOrdersTask" type="softwareElement" internalExceptionHandler="ignore" description="GA EZFUND Order Confirmation Receiver" />
     <eventSource name="GA.BDC.Console.TaskExecutor.Tasks.ReceivePaymentsTask" type="softwareElement" internalExceptionHandler="ignore" description="GA EZFUND Payment Confirmation Receiver" />
