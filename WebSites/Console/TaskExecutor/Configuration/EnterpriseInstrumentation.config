   </eventSources>
   <filterBindings>
      <eventSourceRef name="GA.BDC.Console.TaskExecutor.BusinessLogic.FRIDocPublisher">
         <filterRef name="defaultSoftwareElementFilter" />
      </eventSourceRef>

      <eventSourceRef name="GA.BDC.Console.TaskExecutor.Tasks.ReceiveOrdersTask">
         <filterRef name="defaultSoftwareElementFilter" />
      </eventSourceRef>

      <eventSourceRef name="GA.BDC.Console.TaskExecutor.Tasks.ReceivePaymentsTask">
         <filterRef name="defaultSoftwareElementFilter" />
      </eventSourceRef>

     <eventSourceRef name="GA.BDC.Console.TaskExecutor.BusinessLogic.EZIDocPublisher">
       <filterRef name="defaultSoftwareElementFilter" />
     </eventSourceRef>


      <eventSourceRef name="SWCorporate.SAP.Shared.IDocMessageParser">
         <filterRef name="defaultSoftwareElementFilter" />
      </eventSourceRef>

      <eventSourceRef name="SWCorporate.SAP.Shared.RfcServerHost">
         <filterRef name="defaultSoftwareElementFilter" />
      </eventSourceRef>
      <eventSourceRef name="SWCorporate.SystemEx.Messaging.MessageRouter">
         <filterRef name="defaultSoftwareElementFilter" />
      </eventSourceRef>
      <eventSourceRef name="SWCorporate.SystemEx.Messaging.MessageQueueConfiguredEnumerator">
         <filterRef name="defaultSoftwareElementFilter" />
      </eventSourceRef>
      <eventSourceRef name="SWCorporate.SystemEx.Messaging.FileSystemWatcherAgent">
         <filterRef name="defaultSoftwareElementFilter" />
      </eventSourceRef>
      <eventSourceRef name="SWCorporate.SystemEx.Messaging.MessageQueueConnectionFactory">
         <filterRef name="defaultSoftwareElementFilter" />
      </eventSourceRef>
      <eventSourceRef name="SWCorporate.SystemEx.Messaging.MessageQueueListener">
         <filterRef name="defaultSoftwareElementFilter" />
      </eventSourceRef>
      <eventSourceRef name="SWCorporate.SystemEx.Messaging.MessageQueueFactory">
         <filterRef name="defaultSoftwareElementFilter" />
      </eventSourceRef>
      <eventSourceRef name="SWCorporate.SystemEx.Messaging.MessageQueueListenerAgent">
         <filterRef name="defaultSoftwareElementFilter" />
      </eventSourceRef>
      <eventSourceRef name="SWCorporate.SystemEx.Messaging.FileSystemWatcherDispatch">
         <filterRef name="defaultSoftwareElementFilter" />
      </eventSourceRef>
      <eventSourceRef name="Application">
         <filterRef name="defaultSoftwareElementFilter" />
      </eventSourceRef>
   </filterBindings>
   <filters>
      <filter name="defaultSoftwareElementFilter" description="A default filter for the Software Element event sources." >
         <eventCategoryRef name="Error Message Event">
            <eventSinkRef name="logSink" />
            <eventSinkRef name="smtpSink" />
         </eventCategoryRef>
         <eventCategoryRef name="Trace Message Event">
            <eventSinkRef name="logSink" />
         </eventCategoryRef>
      </filter>
      <filter name="defaultRequestFilter" description="A default filter for the Request event sources." />
   </filters>
   <eventSinks>
      <eventSink name="wmiSink" description="Outputs events to WMI." type="Microsoft.EnterpriseInstrumentation.EventSinks.WmiEventSink" />
      <eventSink name="traceSink" description="Outputs events to the Windows Event Trace." type="Microsoft.EnterpriseInstrumentation.EventSinks.TraceEventSink">
         <parameter name="sessionName" value="TraceSession" />
      </eventSink>
      <eventSink name="logSink" description="Outputs events to the Windows Event Log." type="Microsoft.EnterpriseInstrumentation.EventSinks.LogEventSink">
         <parameter name="entryTypeFieldName" value="EventLogEntryTypeID" />
         <parameter name="defaultEntryType" value="Information" />
         <parameter name="machineName" value="." />
      </eventSink>
      <eventSink name="smtpSink"
                 description="Outputs events to SMTP Mail."
                 type="SWCorporate.SystemEx.Instrumentation.EventSinks.SmtpEventSink, SWCorporate.SystemEx.Instrumentation">
         <parameter name="smtpServer" value="SMTP.appservices.int" />
         <parameter name="from" value="netsvc00@swgao.com" />
         <parameter name="to" value="bdcdevelopers@fundraising.com" />
      </eventSink>
   </eventSinks>
   <eventCategories>
      <eventCategory name="All Events" description="A category that contains all events.">
         <event type="System.Object" />
      </eventCategory>
      <eventCategory name="Standard Events" description="A category containing all sample schema events.">
