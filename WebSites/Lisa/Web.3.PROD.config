<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <swCorporate.systemEx.messaging xdt:Transform="Replace">
    <instrumentation applicationName="GA.BDC.Web.Lisa"
                     messageQueueHost="SQLP08"
                     messageQueueAceTrusteeList="Trusted Corporate PROD Messaging Principals" />
  </swCorporate.systemEx.messaging>
  <appSettings>
    <add key="fundraising.webapi.host" value="https://api.fundraising.com" xdt:Locator="Match(key)" xdt:Transform="SetAttributes" />
    <add key="core.webapi.host" value="https://core.fundraising.com" xdt:Locator="Match(key)" xdt:Transform="SetAttributes" />
    <add key="security.webapi.host" value="https://security.fundraising.com" xdt:Locator="Match(key)" xdt:Transform="SetAttributes" />
    <add key="reports.webapi.host" value="https://reports.fundraising.com" xdt:Locator="Match(key)" xdt:Transform="SetAttributes" />
  </appSettings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
    <customErrors mode="On" xdt:Transform="SetAttributes" />
  </system.web>
</configuration>