# Business project make file, QSPFulfillment Project

SLNDIR	= D:\webprojects\QSPFulfillment
PRJDIR	= $(SLNDIR)\Business
FRMWRK	= C:\WINNT\Microsoft.NET\Framework\v1.1.4322
COMP	= $(FRMWRK)\csc.exe
TARGET	= /target:library /warn:4
DREF	= /lib:$(PRJDIR)\bin\Debug\ /reference:Common.dll,DAL.dll # debug project references
RREF	= /lib:$(PRJDIR)\bin\Release\ /reference:Common.dll,DAL.dll # release project references
REF	= /lib:$(FRMWRK)\ /reference:System.dll,System.Data.dll,System.Xml.dll #system references
DEBUG	= /out:$(PRJDIR)\bin\Debug\Business.dll /debug:full /filealign:4096 #/doc:$(PRJDIR)\bin\Debug\QSPFulfillment.Business.xml 
RELEASE	= /out:$(PRJDIR)\bin\Release\Business.dll /optimize+ #/doc:$(PRJDIR)\bin\Release\QSPFulfillment.Business.xml 

FILES	= $(PRJDIR)\Account.cs
FILES	= $(FILES) $(PRJDIR)\AccountList.cs
