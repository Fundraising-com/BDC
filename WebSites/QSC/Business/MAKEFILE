FILES	= $(FILES) $(PRJDIR)\ContactMaintenance.cs
FILES	= $(FILES) $(PRJDIR)\DistributionCenter.cs
FILES	= $(FILES) $(PRJDIR)\Enums.cs
FILES	= $(FILES) $(PRJDIR)\EstimatedSalesView.cs
FILES	= $(FILES) $(PRJDIR)\FieldManagerList.cs
