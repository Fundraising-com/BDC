FILES	= $(FILES) $(PRJDIR)\GiftOrderReturned.cs
FILES	= $(FILES) $(PRJDIR)\OnlineStore.cs
FILES	= $(FILES) $(PRJDIR)\OnlineStore_AccountReportingUser.cs
FILES	= $(FILES) $(PRJDIR)\Payments.cs
FILES	= $(FILES) $(PRJDIR)\Phone.cs
