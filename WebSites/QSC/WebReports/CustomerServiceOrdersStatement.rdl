  <Code>Private Shared Payment= 0
Private Shared paymentInstance=0
Public Shared Function SetPayment(ByVal CPHInstance As integer, Amount As Double)
    	if (paymentInstance &lt;&gt; CPHInstance )
	
	Payment+= Amount
	
	end if
	paymentInstance = CPHInstance

   SetPayment= Payment
End Function 

Public Shared Function ResetPayment()
   Payment= 0
End Function</Code>
