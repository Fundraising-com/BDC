WithGroup></TablixMember><TablixMember><KeepWithGroup>After</KeepWithGroup></TablixMember><TablixMember><Group Name="Details" /></TablixMember></TablixMembers></TablixRowHierarchy><DataSetName>QSP</DataSetName><Height>0.75in</Height><Width>15.20833in</Width><Style><Border><Style>None</Style></Border></Style></Tablix></ReportItems><Height>2in</Height><Style /></Body><Width>19.77083in</Width><Page><LeftMargin>1in</LeftMargin><RightMargin>1in</RightMargin><TopMargin>1in</TopMargin><BottomMargin>1in</BottomMargin><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="QSP"><ConnectionProperties><DataProvider>SQL</DataProvider><ConnectString>Data Source=GASQLP02;Initial Catalog=QSPFulfillment</ConnectString></ConnectionProperties><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">DataBase</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">80c59739-7605-434a-8395-ecb4987a3d1a</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="QSP"><Query><DataSourceName>QSP</DataSourceName><CommandText>
SELECT  DATENAME(dw , OrderDate_Prior) AS 'DayOfWeek_Prior', OrderDate_Prior, 
SUM(OrderAmtGA_Prior) AS 'OrderAmtGA_Prior', SUM(OrderCntGA_Prior) AS 'OrderCntGA_Prior',
DATENAME(dw , OrderDate_Current) AS 'DayOfWeek_Current',
OrderDate_Current, SUM(OrderAmt_Current) AS 'OrderAmt_Current', SUM(OrderCnt_Current) AS 'OrderCnt_Current'
FROM 
(
SELECT CONVERT(date,DATEADD(year,-1,DATEADD(day,0,coa.TransactionDateTime)),101) AS 'OrderDate_Prior',
0 as 'OrderAmtGA_Prior',
0 AS 'OrderCntGA_Prior',
CONVERT(date,coa.TransactionDateTime,101) AS 'OrderDate_Current',
SUM(coa.Amount) AS 'OrderAmt_Current',
COUNT(*) AS 'OrderCnt_Current'
from GA.core.customerorder co 
join GA.ar.CustomerEnvelopeCustomerOrder ceco  on ceco.CustomerOrderID = co.CustomerOrderID
join GA.ar.CustomerOrderAR coa  on coa.CustomerEnvelopeID = ceco.CustomerEnvelopeID
join GA.core.Customer c  on c.CustomerID = co.CustomerID
join GA.core.Tote t with(nolock) on t.ToteID = co.ToteIDContract
join GA.core.Contract ct on ct.ContractID = t.ContractID
WHERE co.FormCode IN ('000A','0009','000B') and co.Created &gt;= '2014-07-01' 
and co.CustomerOrderStateID not in (0,22)
and coa.isreversal = 0 
AND coa.transactiondatetime &gt;= '2014-07-01' 
AND coa.TransactionDateTime &lt; dateadd(day, datediff(day, 0, getdate()), 0)  --DATE IS LESS THAN TODAY
AND co.CustomerOrderID &gt; 50000000 
and ct.DivisionCode IN (40,41)
AND coa.CustomerOrderARStateID &lt;&gt; 4
group by CONVERT(date,DATEADD(year,-1,DATEADD(day,0,coa.TransactionDateTime)),101), CONVERT(date,coa.TransactionDateTime,101)

UNION ALL 

SELECT CAST(FLOOR(CAST(o.create_date AS FLOAT))AS DATETIME) AS 'OrderDate_Prior',
SUM(od.price*od.quantity) AS 'OrderAmtGA_Prior',
COUNT(DISTINCT o.order_id) AS 'OrderCntGA_Prior',
CAST(FLOOR(CAST(DATEADD(year,1,DATEADD(day,0,o.create_date)) AS FLOAT))AS DATETIME) AS 'OrderDate_Current',
0 AS 'OrderAmt_Current',
0 AS 'OrderCnt_Current'
FROM QSPFulfillment.dbo.[order] as o
      INNER JOIN QSPFulfillment.dbo.[order_detail] as od
            ON od.order_id = o.order_id
      INNER JOIN QSPEcommerce.dbo.cart as cart
            ON cart.x_order_id = o.order_id
      INNER JOIN qspecommerce.dbo.site site
            ON site.site_id = cart.site_id
      INNER JOIN [QSPFulfillment].[dbo].[catalog_item_detail] as cid
            ON od.catalog_item_detail_id = cid.catalog_item_detail_id
    INNER JOIN [QSPFulfillment].[dbo].[catalog_item] as ci
            ON cid.catalog_item_id = ci.catalog_item_id
    INNER JOIN [QSPFulfillment].[dbo].[Product] p
            ON ci.product_id = p.Product_id
WHERE p.Product_Type_id in (1,13,8,9,14,18,19,20)
  AND o.create_date &gt;= '2013-07-01'
  and o.create_date &lt;= '2014-07-01 23:59:59.000'
  AND o.deleted = 0
  AND od.deleted = 0    
  AND o.order_status_id &gt; 90
  AND site.site_id in (4,15)
group by CAST(FLOOR(CAST(o.create_date AS FLOAT))AS DATETIME), 
CAST(FLOOR(CAST(DATEADD(year,1,DATEADD(day,0,o.create_date)) AS FLOAT))AS DATETIME)
) AS ORDERDATA
GROUP BY DATENAME(dw , OrderDate_Prior), OrderDate_Prior, OrderDate_Current
ORDER BY OrderDate_Prior</CommandText></Query><Fields><Field Name="DayOfWeek_Prior"><DataField>DayOfWeek_Prior</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="OrderDate_Prior"><DataField>OrderDate_Prior</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.DateTime</rd:TypeName></Field><Field Name="OrderAmtGA_Prior"><DataField>OrderAmtGA_Prior</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="OrderCntGA_Prior"><DataField>OrderCntGA_Prior</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="DayOfWeek_Current"><DataField>DayOfWeek_Current</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="OrderDate_Current"><DataField>OrderDate_Current</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.DateTime</rd:TypeName></Field><Field Name="OrderAmt_Current"><DataField>OrderAmt_Current</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="OrderCnt_Current"><DataField>OrderCnt_Current</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field></Fields></DataSet></DataSets><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Inch</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">a8314313-ac23-4e3c-9106-4d86f749c8b7</rd:ReportID></Report>