      <Query>
        <DataSourceName>QSPCanadaOrderManagement</DataSourceName>
        <CommandText>SELECT     A.*
	, B.Date As 'BatchDate'
	, B.Id As 'BatchId'
	, 1 As 'ReportTypeForPickList'
	, null As 'ReportStartDate'
	, null As 'ReportEndDate'
	, B.CampaignId As 'CampaignId'
	, B.AccountId As 'AccountId'
	, null As 'InvoiceId'
	, null As 'FMID'
FROM         QSPCanadaOrderManagement..ReportRequest A
	INNER JOIN QSPCanadaOrderManagement..Batch B ON A.BatchOrderId = B.OrderId
WHERE 
	A.Id = @ReportRequestId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ReportRequestId">
            <Value>=Parameters!ReportRequestId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
