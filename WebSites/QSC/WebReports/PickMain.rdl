  <Code>
function ReportsIncluded(b1 As Boolean, b2 As Boolean, b3 As Boolean, b5 As Boolean, b6 As Boolean, b7 As Boolean, b8 As Boolean, b9 As Boolean, b11 As Boolean, b12 As Boolean)

	dim sText as string

	if b1 Then
		sText = sText &amp; "Order Control Sheet" &amp; vbCrLf
	end if

	if b2 Then
		sText = sText &amp; "Pick List" &amp; vbCrLf
	end if

	if b3 Then
		sText = sText &amp; "Confirm Shipment Report" &amp; vbCrLf
	end if

	if b5 Then
		sText = sText &amp; "Participant Listing" &amp; vbCrLf
	end if

	if b6 Then
		sText = sText &amp; "Homeroom Summary Report" &amp; vbCrLf
	end if

	if b7 Then
		sText = sText &amp; "Group Summary Report" &amp; vbCrLf
	end if

	if b8 Then
		sText = sText &amp; "Magazine Item Summary Report" &amp; vbCrLf
	end if

	if b9 Then
		sText = sText &amp; "Problem Solver Report" &amp; vbCrLf
	end if

	if b11 Then
		sText = sText &amp; "Order Entry Followup Report" &amp; vbCrLf
	end if

	if b12 Then
		sText = sText &amp; "Price Discrepancy Report" &amp; vbCrLf
	end if

	ReportsIncluded = sText &amp; ""

end function</Code>
