				"U"c, "V"c, "W"c, "X"c, "Y"c, "Z"c, "-"c, "."c, " "c, "$"c, _
				"/"c, "+"c, "%"c}

Public Shared Function GetCheckSum(ByVal text As String) As Char
	Dim cSum As Integer
	Dim charArray As Char()

	cSum = 0

	charArray = text.ToCharArray()

	For Each c As Char In charArray
		cSum = cSum + GetCharacterValue(c)
	Next

	GetCheckSum = GetCharacter(cSum Mod 43)
End Function

Public Shared Function GetCharacter(ByVal Value As Integer) As Char
	GetCharacter = charTable(Value)
End Function

Public Shared Function GetCharacterValue(ByVal code39Character As Char) As Integer
	GetCharacterValue = Array.IndexOf(charTable, code39Character)
End Function</Code>
