<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="LetterBatchDataSet" targetNamespace="http://tempuri.org/LetterBatchDataSet.xsd" elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/LetterBatchDataSet.xsd" xmlns:mstns="http://tempuri.org/LetterBatchDataSet.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:msprop="urn:schemas-microsoft-com:xml-msprop">
