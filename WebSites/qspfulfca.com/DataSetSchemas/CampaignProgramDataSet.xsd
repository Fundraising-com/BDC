<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="CampaignProgramDataSet" targetNamespace="http://tempuri.org/CampaignProgramDataSet.xsd"
	elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/CampaignProgramDataSet.xsd"
	xmlns:mstns="http://tempuri.org/CampaignProgramDataSet.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:msprop="urn:schemas-microsoft-com:xml-msprop">
	<xs:element name="CampaignProgramDataSet" msdata:IsDataSet="true" msprop:BusinessRule1="CampaignProgramGroupProfitRange"
		msprop:BusinessRule2="CampaignProgramCombinations">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="CampaignProgram">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CampaignID" type="xs:int" />
							<xs:element name="ProgramID" type="xs:int" />
							<xs:element name="IsPreCollect" type="xs:string" minOccurs="0" msprop:nullValue="N" default="N" />
							<xs:element name="GroupProfit" type="xs:decimal" minOccurs="0" msprop:nullValue="-1.0" default="0.0" />
							<xs:element name="DeletedTF" type="xs:boolean" default="false" />
							<xs:element name="IsPersonalize" type="xs:boolean" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Program">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ID" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="Country" type="xs:string" minOccurs="0" msprop:nullValue="CA" />
							<xs:element name="FundraisingProcedureID" type="xs:int" minOccurs="0" msprop:nullValue="0" />
							<xs:element name="ProgramTypeID" type="xs:int" minOccurs="0" msprop:nullValue="0" />
							<xs:element name="Name" type="xs:string" minOccurs="0" msprop:nullValue="_empty" />
							<xs:element name="MajorProductLineID" type="xs:int" minOccurs="0" msprop:nullValue="0" />
							<xs:element name="DefaultProfit" type="xs:decimal" minOccurs="0" msprop:nullValue="-1.0" />
							<xs:element name="MinProfit" type="xs:decimal" minOccurs="0" msprop:nullValue="0.0" />
							<xs:element name="MaxProfit" type="xs:decimal" minOccurs="0" msprop:nullValue="0.0" />
							<xs:element name="OrdefuPrintInAR" type="xs:string" minOccurs="0" msprop:nullValue="_empty" />
							<xs:element name="ActiveForFiscal_TF" type="xs:boolean" />
							<xs:element name="Abr" type="xs:string" minOccurs="0" msprop:nullValue="_empty" />
							<xs:element name="FrenchName" type="xs:string" minOccurs="0" />
							<xs:element name="PrintInvoice" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
		<xs:unique name="CampaignProgramDataSetKey1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:CampaignProgram" />
			<xs:field xpath="mstns:CampaignID" />
			<xs:field xpath="mstns:ProgramID" />
		</xs:unique>
		<xs:unique name="CampaignProgramDataSetKey2" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Program" />
			<xs:field xpath="mstns:ID" />
		</xs:unique>
		<xs:keyref name="ProgramCampaignProgram" refer="CampaignProgramDataSetKey2">
			<xs:selector xpath=".//mstns:CampaignProgram" />
			<xs:field xpath="mstns:ProgramID" />
		</xs:keyref>
	</xs:element>
</xs:schema>